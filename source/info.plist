# -*- coding: utf-8 -*-

from Feedback import Feedback
import sys
import json
import urllib2

reload(sys)
sys.setdefaultencoding('utf8')

query = '{query}'
url = 'https://www.npmjs.com/search/suggestions?q=' + query

req = urllib2.Request(url)
f = urllib2.urlopen(req)

content = f.read()

response = json.loads(content)
fb = Feedback()
try:
    for item in response:
        if item['name'] is not None:
            fb.add_item(item['name'],
                        subtitle=item['description'] + ". v" + item['version'],
                        arg=item['links']['homepage'],
                        icon='icon.png')
except TypeError as te:
    fb.add_item(response['error'], icon='icon.png')
except Exception as e:
    fb.add_item('出错了,请重试！' + e.message)
print fb
